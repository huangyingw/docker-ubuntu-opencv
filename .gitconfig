[remote "hyubs"]
    url = https://github.com/hyubs/docker-ubuntu-opencv.git
    fetch = +refs/heads/*:refs/remotes/hyubs/*
[remote "github"]
    url = https://github.com/huangyingw/docker-ubuntu-opencv.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = hyubs
    branch = master
[merge]
	allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = hyubs
    merge = refs/heads/master
[commit]
	message = n
